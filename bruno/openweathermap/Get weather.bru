meta {
  name: Get weather
  type: http
  seq: 2
}

get {
  url: https://api.openweathermap.org/data/3.0/onecall?lat={{lat}}&lon={{lon}}&exclude={{part}}&appid={{apiKey}}&lang=sv&units=metric
  body: none
  auth: none
}

params:query {
  lat: {{lat}}
  lon: {{lon}}
  exclude: {{part}}
  appid: {{apiKey}}
  lang: sv
  units: metric
}
